{
  "version": 2,
  "name": "school-demo",
  "builds": [
    {
      "src": "frontend/vite.config.js",
      "use": "@vercel/static",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/frontend/(.*)",
      "dest": "/",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
